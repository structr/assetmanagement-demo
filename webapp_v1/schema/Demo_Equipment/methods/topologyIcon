{

	const self = $.this;
	
	let color;
	if(self.status === 'VALID') {
		color = '#00b300';
	} else if(self.status === 'INVALID') {
		color = '#ff0000';
	} else {
		color = '#808080';
	}
	
	return {
		face  : "'Font Awesome 5 Free'",
		code  : '\uf085',
		size  : 50,
		color : color,
		weight: 'bold'
	};

}